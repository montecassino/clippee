{
  "$schema": "https://turborepo.dev/schema.json",
  "ui": "tui",
  "tasks": {
    "install:python": {
      "cache": false
    },
    "db:dev:push": {
      "cache": false,
      "inputs": ["prisma/schema.prisma"]
    },
    "db:dev:generate": {
      "cache": false,
      "inputs": ["prisma/schema.prisma"]
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
